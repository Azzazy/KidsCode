<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mohamedazzazy.test.kidscode" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StatActivity"
            android:label="@string/title_activity_stat" >
        </activity>
        <activity
            android:name=".NewKidActivity"
            android:label="@string/title_activity_new_kid" >
        </activity>
        <activity
            android:name=".ActionsActivity"
            android:label="@string/title_activity_actions" >
        </activity>
        <activity
            android:name=".CoinsActivity"
            android:label="@string/title_activity_coins"
            android:noHistory="true" >
        </activity>
        <activity
            android:name=".NormalAttActivity"
            android:label="@string/title_activity_normal_att"
            android:noHistory="true" >
        </activity>
        <activity
            android:name=".ShowCoinActivity"
            android:label="@string/title_activity_show_coin"
             >
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings_activity"
            android:noHistory="true" >
        </activity>



        <activity
            android:name=".KidInfoActivity"
            android:label="@string/title_activity_kid_info"
            android:noHistory="true">
        </activity>
        <service
            android:name=".java.DB"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service
            android:name=".java.Rearrange"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service
            android:name=".java.ReadFullDB"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>
